<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="neg.Persona" table="PERSONA">
		<id name="intId" column="ID">
			<generator class="native" />
		</id>
		<property name="strNombres" column="nombres" />
		<property name="strApellidos" column="apellidos" />
		<property name="intEdad" column="edad" />
		<property name="strEstado" column="estado" />
		<many-to-one name="objEmpresa" 
			class="neg.Empresa" 
			column="empresa"/>
	</class>
</hibernate-mapping>
